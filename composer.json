{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.0.*",
    "teepluss/theme": "~2.0",
    "laracasts/presenter": "0.2.*",
    "laravel/socialite": "v2.0.1",
    "illuminate/html": "~5.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "~2.0",
    "doctrine/dbal": "~2.3",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database",
      "app/Exceptions/Exceptions.php"
    ],
    "psr-4": {
      "app\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
